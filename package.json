{
  "name": "@quantfive/codepress-engine",
  "version": "0.3.9",
  "description": "CodePress engine - Babel and SWC plug-ins",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./babel": "./babel/index.js",
    "./swc": "./swc/index.js",
    "./swc/wasm": "./swc/codepress_engine.wasm",
    "./hash-util": "./dist/hash-util.js",
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.js"
    },
    "./cli": "./dist/cli.js"
  },
  "bin": {
    "codepress": "./dist/cli.js"
  },
  "files": [
    "babel/",
    "swc/",
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && npm run build:legacy",
    "build:legacy": "babel src --out-dir dist --copy-files --extensions \".ts,.js\"",
    "build:ts": "tsc",
    "build:rust": "mkdir -p swc && cargo build --release --target wasm32-wasip1 --manifest-path codepress-swc-plugin/Cargo.toml && cp codepress-swc-plugin/target/wasm32-wasip1/release/codepress_swc_plugin.wasm swc/codepress_engine.wasm",
    "test": "jest",
    "lint": "eslint src/ babel/ test/ tests/ --ext .js,.ts",
    "lint:fix": "eslint src/ babel/ test/ tests/ --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"babel/**/*.js\" \"test/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" \"*.js\" \"*.md\" \"*.ts\"",
    "format:check": "prettier --check \"src/**/*.{js,ts}\" \"babel/**/*.js\" \"test/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" \"*.js\" \"*.md\" \"*.ts\"",
    "quality": "npm run lint && npm run format:check && npm run build:ts && npm test",
    "dev:link": "npm run build && npm run build:rust && npm link",
    "prepublishOnly": "npm run quality && npm run build && npm run build:rust",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx",
    "react",
    "codepress"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantfive/codepress-engine.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.1",
    "@swc/core": "^1.11.31",
    "@swc/wasm": "^1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/node-fetch": "^2.6.13",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "fastify": "^5.3.3",
    "node-fetch": "^2.6.7",
    "prettier": "^3.1.0"
  }
}
