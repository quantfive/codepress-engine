{
  "name": "@quantfive/codepress-engine",
  "version": "0.3.9",
  "description": "CodePress engine - Babel and SWC plug-ins",
  "main": "./babel/index.js",
  "exports": {
    ".": "./babel/index.js",
    "./babel": "./babel/index.js",
    "./swc": "./swc/index.js",
    "./swc/wasm": "./swc/codepress_engine.wasm",
    "./hash-util": "./dist/hash-util.js",
    "./server": "./dist/server.js",
    "./cli": "./dist/cli.js"
  },
  "bin": {
    "codepress": "./dist/cli.js"
  },
  "files": [
    "babel/",
    "swc/",
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "build:rust": "mkdir -p swc && cargo build --release --target wasm32-wasip1 --manifest-path codepress-swc-plugin/Cargo.toml && cp codepress-swc-plugin/target/wasm32-wasip1/release/codepress_swc_plugin.wasm swc/codepress_engine.wasm",
    "test": "jest",
    "lint": "eslint src/ babel/ test/ tests/ --ext .js",
    "lint:fix": "eslint src/ babel/ test/ tests/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"babel/**/*.js\" \"test/**/*.js\" \"tests/**/*.js\" \"*.js\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.js\" \"babel/**/*.js\" \"test/**/*.js\" \"tests/**/*.js\" \"*.js\" \"*.md\"",
    "quality": "npm run lint && npm run format:check && npm test",
    "dev:link": "npm run build && npm run build:rust && npm link",
    "prepublishOnly": "npm run quality && npm run build && npm run build:rust",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx",
    "react",
    "codepress"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantfive/codepress-engine.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@swc/core": "^1.11.31",
    "@swc/wasm": "^1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "fastify": "^5.3.3",
    "node-fetch": "^2.6.7",
    "prettier": "^3.1.0"
  }
}
