{
  "name": "@quantfive/codepress-engine",
  "version": "0.3.19",
  "description": "CodePress engine - Babel and SWC plug-ins",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./babel": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./hash-util": {
      "types": "./dist/hash-util.d.ts",
      "require": "./dist/hash-util.js",
      "default": "./dist/hash-util.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "require": "./dist/cli.js",
      "default": "./dist/cli.js"
    },
    "./swc": {
      "types": "./dist/swc/index.d.ts",
      "require": "./dist/swc/index.js",
      "default": "./dist/swc/index.js"
    },
    "./swc/wasm": "./swc/codepress_engine.v42.wasm",
    "./swc/wasm-v42": "./swc/codepress_engine.v42.wasm",
    "./swc/wasm-v26": "./swc/codepress_engine.v26.wasm",
    "./swc/wasm-v0_82_87": "./swc/codepress_engine.v0_82_87.wasm"
  },
  "bin": {
    "codepress": "./dist/cli.js"
  },
  "files": [
    "swc/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:rust": "node ./scripts/build-swc.mjs",
    "prepack": "npm run build && npm run build:rust",
    "test": "jest",
    "dev:link": "npm run build && npm run build:rust && npm link",
    "prepublishOnly": "npm run build && npm run build:rust",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx",
    "react",
    "codepress"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantfive/codepress-engine.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@swc/core": "^1.11.31",
    "@swc/wasm": "^1",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.11",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "fastify": "^5.3.3",
    "node-fetch": "^2.6.7",
    "prettier": "^3.1.0",
    "ws": "^8.18.0"
  }
}
