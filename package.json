{
  "name": "@quantfive/codepress-engine",
  "version": "0.8.0",
  "description": "Babel plugin that adds file identifiers to all JSX elements and repository/branch info to container elements like div, html or body",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./hash-util": "./dist/hash-util.js",
    "./server": "./dist/server.js",
    "./cli": "./dist/cli.js"
  },
  "bin": {
    "codepress": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "jsx",
    "react",
    "codepress"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.5.3"
  }
}
